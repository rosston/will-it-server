{
  "name": "will-it-server",
  "version": "0.1.0",
  "description": "More to come",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig-esm.json",
    "example-dev": "npm run build && cd examples/next-15 && npm run dev",
    "example-start": "npm run build && cd examples/next-15 && npm run build&& npm start",
    "format": "eslint --fix",
    "lint": "eslint",
    "test": "npm run test:unit && npm run test:types",
    "test:unit": "teenytest",
    "test:types": "tsd --typings src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosston/will-it-server.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rosston/will-it-server/issues"
  },
  "homepage": "https://github.com/rosston/will-it-server#readme",
  "devDependencies": {
    "@types/react": "^19.1.15",
    "eslint": "^9.36.0",
    "neostandard": "^0.12.2",
    "teenytest": "^6.0.5",
    "teenytest-promise": "^1.0.0",
    "ts-node": "^10.9.2",
    "tsd": "^0.33.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "teenytest": {
    "helper": "test/helper.js",
    "plugins": [
      "teenytest-promise"
    ],
    "testLocator": "test/**/*.test.{js,ts}"
  },
  "tsd": {
    "directory": "test/types"
  }
}
